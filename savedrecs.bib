@article{ ISI:000186671900011,
Author = {Yen, WCK},
Title = {{The bottleneck independent domination on the classes of bipartite graphs
   and block graphs}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2003}},
Volume = {{157}},
Pages = {{199-215}},
Month = {{DEC}},
Abstract = {{Let G(V, E, W) denote a graph with vertex-set V and edge-set E, and each
   vertex v is associated with a cost W(v). For any set V' subset of or
   equal to V, the bottleneck cost of V' is defined as
   max\{W(x)\textbackslash{}x is an element of V'\}.
   This paper considers the bottleneck independent dominating set problem
   (the BIDS problem) which determines an independent dominating set of G
   such that its bottleneck cost is minimized. This paper studies the
   problem on the classes of bipartite graphs and block graphs. This paper
   first proves that the problem is NP-hard on chordal-bipartite graphs.
   Second, a linear-time algorithm on convex-bipartite graphs is proposed.
   Next, a linear-time algorithm on trees is designed. Then, we generalize
   the algorithmic result to block graphs. All algorithms are designed by
   the dynamic programming strategy. (C) 2003 Elsevier Inc. All rights
   reserved.}},
