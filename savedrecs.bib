@article{ ISI:000182691600012,
Author = {Kak, S},
Title = {{General qubit errors cannot be corrected}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2003}},
Volume = {{152}},
Pages = {{195-202}},
Month = {{JUN}},
Abstract = {{Error correction in the standard meaning of the term implies the ability
   to correct all small analog errors and some large errors. Examining
   assumptions at the basis of the recently proposed quantum
   error-correcting codes, it is pointed out that these codes can correct
   only a subset of errors, and are unable to correct small phase errors
   which can have disastrous consequences for a quantum computation. This
   shortcomings will restrict their usefulness in real applications. (C)
