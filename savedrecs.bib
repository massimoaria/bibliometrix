@article{ ISI:000182691600013,
Author = {Jun, YB and Kim, HS and Ahn, SS},
Title = {{Special classes of positive implication algebras}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2003}},
Volume = {{152}},
Pages = {{203-215}},
Month = {{JUN}},
Abstract = {{We construct a positive implication algebra PI(A) inherited from a
   chain. For a nonempty subset del of a positive implication algebra,
   using the special set del(a,b), we give a necessary and sufficient
   condition for V to be an implicative filter. We also introduce the
   notion of a V-type positive implication algebra, and prove the
   followings: (1) every del-type positive implication algebra is a
   commutative monoid under the operation (D, but not a group; (2) every
   V-type positive implication algebra is a lower semilattice; (3) in a
   V-type positive implication algebra the class of implicative filters
   coincides with the class of convex subsemigroups with V. Finally we show
   that every V-type positive implication algebra is a semi-Brouwerian
   algebra, and consider the direct product of implicative algebras. (C)
   2003 Elsevier Science Inc. All rights reserved.}},
