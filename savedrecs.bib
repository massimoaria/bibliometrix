@article{ ISI:000171827100003,
Author = {Xie, H},
Title = {{Patterns between interactive intentions and information-seeking
   strategies}},
Journal = {{INFORMATION PROCESSING \& MANAGEMENT}},
Year = {{2002}},
Volume = {{38}},
Number = {{1}},
Pages = {{55-77}},
Month = {{JAN}},
Abstract = {{In order to design information retrieval (IR) systems to support various
   information-seeking strategies across a variety of contexts, it is
   important to explore what leads to the selection of different types of
   information-seeking strategies. This study addresses the issue by
   investigating the relationships between users' information-seeking goals
   and their information-seeking behaviors. The particular focus of this
   study is to specify hierarchical levels of user goals, and to
   investigate the micro-level goal labeled ``interactive intention{''} in
   relation to information-seeking strategies. A total of 40 users from
   four types of libraries were selected for this study, and the analysis
   illustrates how changes in interactive intentions are associated with
   changes of information-seeking strategies. The results present how eight
   types of interactive intentions, four types of entities, eight types of
   methods, and six types of resources constitute the patterns of
   interactive intentions and information-seeking strategies. The
   implications for IR system design are further discussed. (C) 2001
   Elsevier Science Ltd. All rights reserved.}},
