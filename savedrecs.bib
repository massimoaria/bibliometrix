@article{ ISI:000182206300011,
Author = {Chang, W and Park, JB and Joo, YH and Chen, GR},
Title = {{Static output-feedback fuzzy controller for Chen's chaotic system with
   uncertainties}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2003}},
Volume = {{151}},
Pages = {{227-244}},
Month = {{MAY}},
Abstract = {{Static output-feedback stabilization of Chen's chaotic system with
   uncertainties is studied from a fuzzy control approach. An uncertain
   Takagi-Sugeno (TS) fuzzy model for the uncertain Chen's chaotic system
   is first constructed. Then, the uncertain TS fuzzy model is represented
   as a set of uncertain linear systems and a controller design algorithm
   is proposed. Specifically, a switching static output-feedback
   fuzzy-model-based controller is designed by solving some linear matrix
   inequalities (LMI), which are derived from the developed local
   representations. Simulation results have confirmed the effectiveness and
   feasibility of the proposed modeling and control methods. (C) 2002
   Elsevier Science Inc. All rights reserved.}},
