@article{ ISI:000175844100016,
Author = {Resconi, G and van der Wal, AJ},
Title = {{Morphogenic neural networks encode abstract rules by data}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2002}},
Volume = {{142}},
