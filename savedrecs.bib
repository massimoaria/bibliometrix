@article{ ISI:000181217300004,
Author = {Safronov, V and Parashar, M},
Title = {{Optimizing Web servers using Page rank prefetching for clustered
   accesses}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2003}},
Volume = {{150}},
Number = {{3-4}},
Pages = {{165-176}},
Month = {{APR}},
Note = {{4th Workshop on Information and Computer Science, SAUDI ARABIA, MAR
   17-18, 2002}},
Organization = {{Saudi Aramco; Al Alamia; Bugshan; Al Hussan Grp Co}},
Abstract = {{This paper presents a Page rank-based prefetching technique for accesses
   to Web page clusters. The approach uses the link structure of a
   requested page to determine the,'most important{''} linked pages and to
   identify the page(s) to be prefetched. The underlying premise of our
   approach is that in the case of cluster accesses, the next pages
   requested by users of the Web server are typically based on the current
   and previous pages requested. Furthermore, if the requested pages have a
   lot of links to some ``important{''} page, that page has a higher
   probability of being the next one requested. An experimental evaluation
   of the prefetching mechanism is presented using real server logs. The
   results show that the Page rank-based scheme does better than random
   prefetching for clustered accesses, with hit rates of 90\% in some
   cases. (C) 2002 Elsevier Science Inc. All rights reserved.}},
