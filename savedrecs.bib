@article{ ISI:000179530600010,
Author = {Juang, HG},
Title = {{Intelligent locomotion control on sloping surfaces}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2002}},
Volume = {{147}},
Number = {{1-4}},
Pages = {{229-243}},
Month = {{NOV}},
Abstract = {{A learning scheme is developed to generate robotic locomotion on
   different sloping surfaces. This scheme uses three neural networks: a
   neural network controller, a neural network emulator, and a slope
   information neural network. The neural network controller is pre-trained
   by a reference trajectory on a horizontal surface. The emulator is
   pre-trained to identify the robotic dynamics. The slope information
   neural network provides compensated control signals to the robot on
   different slope angles by using the control signals on the horizontal
   surface from the pre-trained controller. The training rule is a
   backpropogation algorithm with time delay. The proposed technique can
   generate gaits on different sloping surfaces by following a reference
   trajectory with desired step length, crossing height and walking speed.
   (C) 2002 Elsevier Science Inc. All rights reserved.}},
