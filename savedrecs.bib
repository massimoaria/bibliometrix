@article{ ISI:000173675300005,
Author = {Nemoto, N and Tanaka, K and Takizawa, M},
Title = {{Quality-based synchronization methods of multimedia objects}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2002}},
Volume = {{141}},
Number = {{1-2}},
Pages = {{81-95}},
Month = {{MAR}},
Abstract = {{It is critical for applications to obtain enough quality of service
   (QoS) from multimedia objects. Not only states but also QoS of objects
   are changed by methods. The objects are manipulated by multiple
   transactions, Here, the objects are required to be consistent. We
   discuss new types of consistency of multimedia objects with respect to
   QoS. Since it takes a longer time to perform a method on multimedia
   objects, the throughput of the system is decreased if objects are
   exclusively locked in traditional concurrently control. We separate a
   concurrency control mechanism to two orthogonal ones, one for
   serializing transactions and the other for mutually exclusive access to
   objects. The timestamp ordering protocol is adopted for the former one
   and the locking protocol for the latter. (C) 2002 Elsevier Science Inc.
   All rights reserved.}},
