@article{ ISI:000183472000006,
Author = {Oussalah, M and De Schutter, J},
Title = {{Approximated fuzzy LR computation}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2003}},
Volume = {{153}},
Pages = {{155-175}},
Month = {{JUL}},
Abstract = {{This note addresses the problem of the multiplication (and division) of
   fuzzy numbers represented in LR (left and right) form. It is known that,
   usually, the multiplication of LR fuzzy numbers does not provide LR
   fuzzy number. However, for real time applications, for instance, the
   complexity of such operation often prevents the use of fuzzy arithmetic
   framework to these fields. This rises the question whether an
   approximated LR fuzzy number can be found in case of fuzzy
   multiplication (division) operation. This paper focuses on an
   approximated LR fuzzy number that agrees with the result supplied by the
   fuzzy multiplication (division) operation according to some rational
   criteria. These criteria involve properties of global specificity, high
   plausibility and typicality representation. Furthermore, the error
   induced by this approximation is evaluated and included as a constraint
   for the approximation issue. (C) 2003 Elsevier Science Inc. All rights
   reserved.}},
